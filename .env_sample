# Vast S3 Configuration
# Copy this file and update with your actual credentials

# Your Vast S3 endpoint URL
VAST_S3_ENDPOINT=https://your-vast-s3-endpoint.example.com

# AWS Access Key ID for your Vast S3 account
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY

# AWS Secret Access Key for your Vast S3 account  
AWS_SECRET_ACCESS_KEY=YOUR_SECRET_KEY

# Default bucket name to use in scripts
VAST_BUCKET_NAME=my-vast-bucket

# Region
export AWS_REGION="us-east-1"
